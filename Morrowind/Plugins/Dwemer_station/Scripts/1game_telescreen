begin 1game_telescreen


;Телепортация по миру

short button
short state

if ( OnActivate == 1 )
    if ( "1game_energy" == 1 )
      set state to 1
    else
      messagebox "Ничего не происходит, кажется, требуется питание"
      return
  endif
endif

